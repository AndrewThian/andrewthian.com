{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "ordered-imports": [ false ],
        "member-access": [ false ],
        "no-console": [ false ],
        "object-literal-sort-keys": [ false ],
        "no-var-requires": true,
        "quotemark": [ true, "double", "jsx-double" ]
    },
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts"
        ]
    }
}